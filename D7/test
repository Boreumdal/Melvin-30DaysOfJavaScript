let randomUserIp = () => {
    let a, b, c, d
    a = Math.floor(Math.random() * 255)
    b = Math.floor(Math.random() * 255)
    c = Math.floor(Math.random() * 255)
    d = Math.floor(Math.random() * 255)
    let proto = `${a}.${b}.${c}.${d}`
    return proto
}
console.log(randomUserIp());


let randomHexaNumberGenerator = () => {
    let hex = '#'
    let giv = 'abcdef0123456789'
    for (i=1;i<=6;i++){
        hex += giv[Math.floor(Math.random() * 15)]
    }
    return hex
}

console.log(randomHexaNumberGenerator());

let userIdGenerator = () => {
    let uid = ''
    let giv = 'AaBbCcDdEeFfGgHhIiJjKkLlMmNnOoPpQqRrSsTtUuVvWwXxYyZz0123456789'
    for (let i = 1; i <= 7; i++){
        uid += giv[Math.floor(Math.random() * 61)]
    }
    return uid
}

console.log(userIdGenerator());

let userIdGenerator = () => {
    let nc = prompt('Number of characters:', '')
    let ni = prompt('Number of ids to be generated:', '')
    let uid = ''
    let giv = 'AaBbCcDdEeFfGgHhIiJjKkLlMmNnOoPpQqRrSsTtUuVvWwXxYyZz0123456789'
    for (let a = 1; a <= ni; a++){
        for (let i = 1; i <= nc; i++){
            uid += giv[Math.floor(Math.random() * 61)]
        }
        uid += '\n'
    }
    return uid
}

console.log(userIdGenerator());

let rgbColorGenerator = () => {
    let r = Math.floor(Math.random() * 255)
    let g = Math.floor(Math.random() * 255)
    let b = Math.floor(Math.random() * 255)
    let rgb = `rgb(${r},${g},${b})`
    return rgb
}

console.log(rgbColorGenerator());


let arrayOfHexaColors = () => {
    let numOfHex = prompt('Number of hex:', '')
    let giv = 'abcdef0123456789'
    let arr = []
    for (let a = 1; a <= numOfHex; a++){
        let hex = '#'
        for (let i = 1; i<=6; i++){
            hex += giv[Math.floor(Math.random() * 15)]
        }
        arr.push(hex)
    }
    return arr
}
console.log(arrayOfHexaColors());

let arrayOfRgbColors = () => {
    let numOfRgb = prompt('Number of rgb:', '')
    let arr = []
    for (let a = 1; a <= numOfRgb; a++){
        let r = Math.floor(Math.random() * 255)
        let g = Math.floor(Math.random() * 255)
        let b = Math.floor(Math.random() * 255)
        let rgb = `rgb(${r},${g},${b})`
        arr.push(rgb)
    }
    return arr
}
console.log(arrayOfRgbColors());

let generateColors = (typ, counts) => {
    if (typ == 'rgb'){
        let result = []
        for (let i = 1; i <= counts; i++){
            let r = Math.floor(Math.random() * 255)
            let g = Math.floor(Math.random() * 255)
            let b = Math.floor(Math.random() * 255)
            let rgb = `rgb(${r},${g},${b})`
            result.push(rgb)
        }
        return result
    }
    else if (typ == 'hexa'){
        let result = []
        for (let i = 1; i <= counts; i++){
            let hex = '#'
            for (let o = 1; o <= 6; o++){
                let chars = 'abcdef1234567890'
                hex += chars[Math.floor(Math.random() * (chars.length-1))]
            }
            result.push(hex)
        }
        return result
    }
}
console.log(generateColors('rgb', 3))
console.log(generateColors('hexa', 3))


let shuffleArray = (samp) => {
    let shuffled = []

    for (let i = 0; i < samp.length; i++){
        let selectorRandom = Math.floor(Math.random() * samp.length)
        shuffled.push(samp[selectorRandom]);
    }    
    return shuffled
}
console.log(shuffleArray(['a','b','c','d','e','a','b','c','d','e','a','b','c','d','e']));


let factorial = (num) => {
    let mult = 1
    for (let i = num; i > 0; i--){
        mult *= i
    }
    return mult
}
console.log(factorial(4));


let isEmpty = (check) => {
    if (check == '' || check == null || check === undefined){
        console.log('Empty');
    }
    else {
        console.log('Not empty');
    }
}

let sum = (...nums) => {
    summary = 0
    for (let i = 0; i < nums.length; i++){
        summary += nums[i]
    }
    return summary
}
console.log(sum(1,2,3,4,5,5));


let sumOfArrayItems = (arr) => {
    summary = 0
    for (const suum of arr){
        if (typeof suum === 'number'){
            summary += suum
        }
        else {
            return 'not'
        }
    }
    return summary
}
console.log(sumOfArrayItems([1,2,3,'4',5,5]))

let average = (arr) => {
    summary = 0
    ave = arr.length
    for (const suum of arr){
        if (typeof suum === 'number'){
            summary += suum
        }
        else {
            return 'not'
        }
    }
    return summary / arr.length
}
console.log(average([1,2,3,4,5,5]));

let modifyArray = (arr) => {
    let modifiedArray = []
    if (arr.length < 5){
        return 'not found'
    }
    else {
        for (let i = 0; i < arr.length; i++){
            if (i == 4){
                let upperr = arr[i]
                modifiedArray.push(upperr.toUpperCase())
            }
            else {
                modifiedArray.push(arr[i])
            }
        }
    }
    return modifiedArray
}
console.log(modifyArray(['Avocado', 'Tomato', 'Potato','Mango', 'Lemon','Carrot']))

let isPrime = (num) => {
    if (num < 1){
        return 'not prime'
    }
    else if (num > 1){
        for (let i = 2; i < num; i++){
            if (num % i == 0){
                return 'not'
            }
        }
    }
    else {
        return 'not prime'
    }
    return 'primeeee'
}   

console.log(isPrime(39));



