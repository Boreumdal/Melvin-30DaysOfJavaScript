let showDateTime = () => {
    let theDate = new Date()
    let mm = theDate.getMonth() + 1
    let dd = theDate.getDay()
    let yy = theDate.getFullYear()
    let min = theDate.getMinutes()
    let hr = theDate.getHours()

    console.log(`${mm}/${dd}/${yy} ${hr}:${min}`);
}

