let reverseArr = arrAy => {
    let arr = []
    for (let i = arrAy.length; i > 0; i--){
        arr.push(arrAy[i-1])
    }
    return arr
}

console.log(reverseArr(['a', 'b', 'c', 'd']));

let capitalizeArray = arrString => {
    let capital1 = arrString.toString()
    let capital2 = capital1.toUpperCase()
    let capitalizedArray = capital2.split(',')

    return capitalizedArray
}

console.log(capitalizeArray(['aasfasfdasf', 'basfdfasfasf', 'casdasfasf', 'dsadfdwsf']))

let customItems = (...items) => {
    
    for (let i = 0; i < items.length; i++){
        item.push(items[i])
    }
    return item
}
console.log(customItems('red', 'bluie', 'whire', 'yellow'));


