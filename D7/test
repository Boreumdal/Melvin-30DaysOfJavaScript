let massIndex = (w, h) => {
    let bmi = w / (h * h)
    if (bmi < 18.5){
        return 'Underweight'
    }
    else if (bmi >= 18.5 && bmi <= 249){
        return 'Normal weight'
    }
    else if (bmi >= 25 && bmi <= 29.9){
        return 'Overweigth'
    }
    else if (bmi >= 30){
        return 'Obese'
    }
    else {
        return 'Enter data'
    }
}

