let uniqueArray = () => {
    let uni = []

    while (uni.length < 7){
        let gen = Math.floor(Math.random() * 7) + 1
        if (uni.indexOf(gen) === -1){
            uni.push(gen)
        }
    }
    return uni
}
console.log(uniqueArray());


